{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": [],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["node_modules/", "dist/"],
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "semi": "off",
    "quotes": "off"
  }
}
